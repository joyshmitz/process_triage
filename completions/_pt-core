'list-backups:List available backup versions' \
'show-backup:Show backup details' \
'verify-backup:Verify a backup'\''s integrity' \
'prune-backups:Remove old backups (keep most recent N)' \
'help:Print this message or the help of the given subcommand(s)' \
    )
    _describe -t commands 'pt-core update commands' commands "$@"
}
