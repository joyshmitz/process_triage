        case $line[1] in
            (plan)
_arguments "${_arguments_options[@]}" : \
'--session=[Resume existing session]:SESSION:_default' \
'--label=[Label for this plan session (e.g. "baseline" for diff --baseline)]:LABEL:_default' \
