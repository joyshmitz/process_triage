'--help[Print help (see more with '\''--help'\'')]' \
'-V[Print version]' \
'--version[Print version]' \
":: :_pt-core__agent__fleet_commands" \
"*::: :->fleet" \
&& ret=0

    case $state in
    (fleet)
        words=($line[1] "${words[@]}")
        (( CURRENT += 1 ))
        curcontext="${curcontext%:*:*}:pt-core-agent-fleet-command-$line[1]:"
        case $line[1] in
            (plan)
_arguments "${_arguments_options[@]}" : \
'(--inventory --discovery-config)--hosts=[Hosts spec (comma-separated list or file path)]:HOSTS:_default' \
'(--hosts --discovery-config)--inventory=[Inventory file path (TOML/YAML/JSON)]:INVENTORY:_default' \
'(--hosts --inventory)--discovery-config=[Discovery config file path (TOML/YAML/JSON)]:DISCOVERY_CONFIG:_default' \
'--parallel=[Max concurrent host connections]:PARALLEL:_default' \
'--timeout=[Per-host timeout (seconds)]:TIMEOUT:_default' \
