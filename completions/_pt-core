(( $+functions[_pt-core_commands] )) ||
_pt-core_commands() {
    local commands; commands=(
'run:Interactive golden path\: scan  infer  plan  TUI approval  staged apply' \
'scan:Quick multi-sample scan only (no inference or action)' \
'deep-scan:Full deep scan with all available probes' \
'diff:Compare two sessions and show differences' \
'query:Query telemetry and history' \
'bundle:Create or inspect diagnostic bundles' \
'report:Generate HTML reports' \
'check:Validate configuration and environment' \
'learn:Interactive tutorials and onboarding guidance' \
'agent:Agent/robot subcommands for automated operation' \
'robot:Agent/robot subcommands for automated operation' \
'config:Configuration management' \
'telemetry:Telemetry management' \
'shadow:Shadow mode observation management' \
'signature:Signature management (list, add, remove user signatures)' \
'schema:Generate JSON schemas for agent output types' \
'update:Update management\: rollback, backup, version history' \
'mcp:MCP server for AI agent integration' \
'completions:Generate shell completion scripts' \
'version:Print version information' \
'help:Print this message or the help of the given subcommand(s)' \
    )
    _describe -t commands 'pt-core commands' commands "$@"
}
(( $+functions[_pt-core__agent_commands] )) ||
_pt-core__agent_commands() {
    local commands; commands=(
'plan:Generate action plan without execution' \
'explain:Explain reasoning for specific candidates' \
'apply:Execute actions from a session' \
'verify:Verify action outcomes' \
'diff:Show changes between sessions' \
'snapshot:Create session snapshot for later comparison' \
'capabilities:Dump current capabilities manifest' \
'sessions:List and manage sessions' \
'list-priors:List current prior configuration' \
'inbox:View pending plans and notifications' \
'tail:Stream session progress events (JSONL)' \
'watch:Watch for new candidates and emit notifications' \
'export-priors:Export priors to file for transfer between machines' \
'import-priors:Import priors from file (bootstrap from external source)' \
'init:Initialize pt for installed coding agents' \
'export:Export session bundle (alias for bundle create)' \
'fleet:Fleet-wide operations across multiple hosts' \
'help:Print this message or the help of the given subcommand(s)' \
    )
    _describe -t commands 'pt-core agent commands' commands "$@"
}
