(( $+functions[_pt-core__agent__help_commands] )) ||
_pt-core__agent__help_commands() {
    local commands; commands=(
'plan:Generate action plan without execution' \
'explain:Explain reasoning for specific candidates' \
'apply:Execute actions from a session' \
'verify:Verify action outcomes' \
'diff:Show changes between sessions' \
'snapshot:Create session snapshot for later comparison' \
'capabilities:Dump current capabilities manifest' \
'sessions:List and manage sessions' \
'list-priors:List current prior configuration' \
'inbox:View pending plans and notifications' \
'tail:Stream session progress events (JSONL)' \
'watch:Watch for new candidates and emit notifications' \
'export-priors:Export priors to file for transfer between machines' \
'import-priors:Import priors from file (bootstrap from external source)' \
'init:Initialize pt for installed coding agents' \
'export:Export session bundle (alias for bundle create)' \
'fleet:Fleet-wide operations across multiple hosts' \
'help:Print this message or the help of the given subcommand(s)' \
    )
    _describe -t commands 'pt-core agent help commands' commands "$@"
}
