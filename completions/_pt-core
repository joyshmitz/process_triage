(( $+functions[_pt-core__signature_commands] )) ||
_pt-core__signature_commands() {
    local commands; commands=(
'list:List all signatures (built-in and user-defined)' \
'show:Show details of a specific signature' \
'add:Add a new user signature' \
'remove:Remove a user signature' \
'test:Test if a process name matches any signature' \
'validate:Validate user signatures file' \
'export:Export signatures to a file' \
'disable:Disable a signature without deleting it' \
'enable:Re-enable a previously disabled signature' \
'import:Import signatures from a file or .ptb bundle' \
'stats:Show signature performance statistics' \
'help:Print this message or the help of the given subcommand(s)' \
    )
    _describe -t commands 'pt-core signature commands' commands "$@"
}
