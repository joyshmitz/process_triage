          "/data/repos/frontend"
        ],
        "trust_level": "user",
        "prior_adjustments": {
          "useful_mult": 1.0,
          "useful_bad_mult": 1.1,
          "abandoned_mult": 1.3,
          "zombie_mult": 1.2
